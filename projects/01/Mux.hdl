/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel, out=ns);
    And(a=a, b=ns, out=o1);
    And(a=sel, b=b, out=o2);
    Or(a=o1, b=o2, out=out);
}
